<section id="main" >
<% for(const singleContent of content) { %>
	<div>
		<% if (singleContent.type === 'article-big') { %>
			<article class="article-big" >
				<div class="article-content">
					<%- singleContent.content %>
				</div>
				<p class="article-date">
					Ver√∂ffentlicht: <%= new Date(singleContent.created).toLocaleString('de-DE', dateOptions) %><% if (singleContent.updated) { %> - Aktualisiert: <%= new Date(singleContent.updated).toLocaleString('de-DE' , dateOptions) %><% } %></p>
			</article>
		<% } %>
	</div>
<% } %>

</section>